# Build-time environment variables for Docker Compose
# Docker Compose automatically loads this file and substitutes ${VAR} in docker-compose.prod.yml
# These are used when building frontend containers (Vite embeds them at build time)

# Google OAuth Client ID (same value as in backend.env)
GOOGLE_CLIENT_ID=your_production_google_client_id

# Frontend API URLs
VITE_API_URL=https://api.epunch.app/v1
VITE_USER_APP_URL=https://epunch.app
VITE_GOOGLE_REDIRECT_URI=https://epunch.app/auth/callback
